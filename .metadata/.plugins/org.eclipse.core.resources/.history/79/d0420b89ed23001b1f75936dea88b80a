package com.learning.tdddemo.classes;

public class Tdd {

	
	 int id;
	 String name;

	public Tdd(int i, String string) {
		id=i;
		name=string;
	}

	public int addNums(int i, int j) {
		// TODO Auto-generated method stub
		return i+j;
	}

	public int subtractNums(int i, int j) {
		// TODO Auto-generated method stub
		return i-j;
	}

	public int divide(int i, int j) {
		// TODO Auto-generated method stub
		return i/j;
	}

}
